<script type="text/javascript">
    var dominio = 'http://tukarta.com/app/';
    function registro() {
        //document.getElementById('loader').style.display = 'block'
        $('#loader').addClass('animated fadeIn').one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function () { $('#btbck').removeClass('animated fadeIn') });
        var a = document.getElementById('psw').value
        var b = document.getElementById('psw2').value
        if (a == b) {
            $.ajax({
                url: dominio + 'registro.php',
                data: $("#form1").serialize(),
                type: "POST",
                timeout: 30000,
                error: function (x, t, m) {
                    if (t === "timeout") {

                    }
                },
                success: function (data) {
                    console.log(data);
                    //document.getElementById('loader').style.display = 'none'
                    if (parseInt(data) == 1) {
                        checkpass("¡Registro con exito!")
                    } else {

                        if (parseInt(data) == "") {
                            errorin("¡Ingrese los campos solicitados!");
                        } else {
                            errorin("¡Ese email ya existe!")
                        }
                    }
                }
            });
        } else {
            errorin("¡Las contraseñas no coinciden!")
        }
    }

    // ----------
    // jQuery Validation
    // ----------
    $.extend($.validator.messages, {
        accept: "Por favor, ingrese un archivo con un formato válido.",
        cifES: "Por favor, escriba un CIF válido.",
        creditcard: "Por favor, escriba un número de tarjeta válido.",
        date: "Por favor, escriba una fecha válida.",
        dateISO: "Por favor, escriba una fecha (ISO) válida.",
        digits: "Por favor, escriba sólo dígitos.",
        email: "Por favor, escriba un correo electrónico válido.",
        equalTo: "Por favor, escriba el mismo valor de nuevo.",
        extension: "Por favor, escriba un valor con una extensión permitida.",
        max: $.validator.format("Por favor, escriba un valor menor o igual a {0}."),
        maxlength: $.validator.format("Por favor, no escriba más de {0} caracteres."),
        min: $.validator.format("Por favor, escriba un valor mayor o igual a {0}."),
        minlength: $.validator.format("Por favor, no escriba menos de {0} caracteres."),
        nieES: "Por favor, escriba un NIE válido.",
        nifES: "Por favor, escriba un NIF válido.",
        number: "Por favor, escriba un número válido.",
        pattern: "Por favor, escriba un formato válido.",
        range: $.validator.format("Por favor, escriba un valor entre {0} y {1}."),
        rangelength: $.validator.format("Por favor, escriba un valor entre {0} y {1} caracteres."),
        remote: "Por favor, llene este campo.",
        required: "Este campo es obligatorio.",
        url: "Por favor, escriba una URL válida.",
        step: "Por favor, ingrese un número entero."
    });



    $(function () {
        let contactForm = $("#contact_form").validate({
            errorClass: "text-danger",
            errorElement: "em",
            submitHandler: function (formElement, e) {
                e.preventDefault();
                let $btn = Ladda.create($(formElement).find("button[type='submit']")[0]);
                $btn.start();
                $.ajax({
                    url: "https://tukarta.com/guardarcontacto.php",
                    data: $(formElement).serialize(),
                    type: "POST",
                    timeout: 30000,
                    complete: function () {
                        $btn.stop();
                    },
                    error: function (x, t, m) {
                        if (t === "timeout") {

                        }
                    },
                    success: function (data) {
                        console.log(data);
                        //document.getElementById('loader').style.display = 'none'
                        if (parseInt(data) == 1) {
                            $("#cta_modal").modal("hide");
                            toastr.success("Sus datos se enviaron con éxito.", "¡Excelente!");
                        } else {
                            toastr.error("Ha ocurrido un problema al intentar guardar sus datos.", "Error");
                        }
                    }
                });
            }
        });
        $("#open_cta_modal").on("click", function (e) {
            contactForm.resetForm();
            $("#cta_modal").modal("toggle");
        });
    });

    function enviar() {
        $('#loader').addClass('animated fadeIn').one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function () { $('#btbck').removeClass('animated fadeIn') });

    }
    function errorin(tiperror) {
        //document.getElementById('loader').style.display = "block";
        document.getElementById('alertn').innerHTML = "<div style='height:20%; overflow:hidden; font-weight:bold; font-size:120%; text-align:left; color:#79919d'>Notificación</div><div style='overflow:hidden; padding: 10% 8% 18% 8%; font-size:130%;'>" + tiperror + "</div><a class='btnn' href='javascript:closen()' style='border: 2px solid #fff; color: #fff; background-color:#536d7a; font-size: 100%; padding: 5% 20%;'>ACEPTAR</a>";
        document.getElementById('alertn').style.display = "block; background-color:#cfd8dd;color:#79919d;";
        $("#alertn").delay(100).show().animate({ opacity: 1, top: "25%" }, 'slow');
    }
    function checkpass(tipcheck) {
        //document.getElementById('loader').style.display = "block";
        document.getElementById('alertp').innerHTML = "<div style='height:20%; overflow:hidden; font-weight:bold; font-size:120%; text-align:left;background-color:#cfd8dd;color:#79919d'>Notificación</div><div style='overflow:hidden; padding: 10% 8% 18% 8%; font-size:130%;'>" + tipcheck + "</div><a class='btnp' href='javascript:closep()' style='border: 2px solid #fff; color: #fff; background-color:#536d7a; font-size: 100%; padding: 5% 20%;margin-left:10%; '>ACEPTAR</a>";
        document.getElementById('alertp').style.display = "block";
        $("#alertp").delay(100).show().animate({ opacity: 1, top: "25%" }, 'slow');

    }
    function closen() {
        //document.getElementById('loader').style.display = "none";
        $("#alertn").delay(100).show().animate({ opacity: 1, top: "-60%" }, 'slow');

    }
    function closep() {
        //document.getElementById('loader').style.display = "none";
        $("#alertp").delay(100).show().animate({ opacity: 1, top: "-60%" }, 'slow');


    }

</script>

